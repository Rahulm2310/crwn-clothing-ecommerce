{
  "name": "estylish-server",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "test-client": "cd client && npm run test"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.17.3",
    "express-sslify": "1.2.0",
    "react-scripts": "^3.4.1",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}
